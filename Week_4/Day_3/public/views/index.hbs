<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Document</title>
  </head>
  <body>
    <h1>Web dev class be like</h1>
    <img src='https://c.tenor.com/uBcbvnuP3K0AAAAd/lil-yachty-drake.gif' alt='yes' />

    <h2>Hello {{name}}</h2>
    <h4>Welcome to the {{course}} bootcamp</h4>

    {{! Helpers }}
    {{#if address}}
      <p>Current location: {{address.street}} {{address.number}}</p>
    {{else}}
      <p>No address provided</p>
    {{/if}}

    {{#unless age}}
      <p>No age provided!</p>
    {{/unless}}

    <ul>
      {{#each cities}}
        <li>{{this}}</li>
      {{else}}
        <p>There are no cities yet!</p>
      {{/each}}

    </ul>

  </body>
</html>